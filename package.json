{
  "name": "olaolu.me",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "fresh": "del dist landing/dist shelf/dist",
    "serve:shelf": "cd shelf && yarn develop",
    "serve:landing": "cd landing && yarn serve",
    "serve": "yarn fresh && node dev.js",
    "build:shelf": "cd shelf && pwd && yarn install && pwd && yarn build",
    "build:landing": "cd landing && pwd && yarn install && pwd && yarn build",
    "build": "yarn fresh && pwd && node build.js && pwd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whizkydee/olaolu.me.git"
  },
  "author": "Olaolu Olawuyi",
  "bugs": {
    "url": "https://github.com/whizkydee/olaolu.me/issues"
  },
  "homepage": "https://github.com/whizkydee/olaolu.me#readme",
  "devDependencies": {
    "del-cli": "^2.0.0",
    "make-dir": "^3.0.0",
    "move-file": "^1.2.0",
    "npm-run-all": "^4.1.5"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/bundlers/vue@2.5.1"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}